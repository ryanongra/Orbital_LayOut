<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.orbital_layoutfrontend">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/layout_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/layout_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:screenOrientation="portrait">
        <activity android:name=".IndivDrillPage"></activity>
        <activity android:name=".AllDrillsPage" />
        <activity android:name=".MatchSummaryPage" />
        <activity android:name=".AnalysePage" />
        <activity android:name=".DrillsPage" />
        <activity android:name=".PlayerPage" />
        <activity android:name=".TrackPage" />
        <activity android:name=".NewPlayerPage" />
        <activity android:name=".GuidePage" />
        <activity android:name=".PlayerSelectPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>